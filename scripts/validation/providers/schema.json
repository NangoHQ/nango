{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "additionalProperties": false,
    "patternProperties": {
        "^[a-z0-9-]+$": {
            "type": "object",
            "additionalProperties": false,
            "required": ["display_name", "docs"],
            "properties": {
                "alias": {
                    "type": "string"
                },
                "display_name": {
                    "type": "string"
                },
                "auth": {
                    "type": "object",
                    "additionalProperties": true,
                    "properties": {
                        "response_type": {
                            "type": "string"
                        }
                    }
                },
                "auth_mode": {
                    "type": "string",
                    "enum": [
                        "API_KEY",
                        "APP",
                        "APP_STORE",
                        "BASIC",
                        "NONE",
                        "OAUTH1",
                        "OAUTH2",
                        "OAUTH2_CC",
                        "CUSTOM",
                        "TBA",
                        "TABLEAU",
                        "JWT",
                        "BILL",
                        "TWO_STEP",
                        "SIGNATURE"
                    ]
                },
                "authorization_method": {
                    "type": "string",
                    "enum": ["header"]
                },
                "authorization_params": {
                    "type": "object",
                    "additionalProperties": true,
                    "properties": {
                        "access_type": {
                            "type": "string",
                            "enum": ["offline"]
                        },
                        "approval_prompt": {
                            "type": "string",
                            "enum": ["auto", "force"]
                        },
                        "duration": {
                            "type": "string",
                            "enum": ["permanent"]
                        },
                        "expiration": {
                            "type": "string",
                            "enum": ["never"]
                        },
                        "force_verify": {
                            "type": "boolean"
                        },
                        "nonce": {
                            "type": "string"
                        },
                        "prompt": {
                            "type": "string",
                            "enum": ["consent"]
                        },
                        "response_mode": {
                            "type": "string"
                        },
                        "response_type": {
                            "type": "string"
                        }
                    }
                },
                "authorization_url": {
                    "type": "string"
                },
                "authorization_url_skip_encode": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "authorization_url_replacements": {
                    "type": "object",
                    "additionalProperties": true
                },
                "body_format": {
                    "type": "string"
                },
                "categories": {
                    "type": "array",
                    "items": {
                        "type": "string",
                        "enum": [
                            "accounting",
                            "analytics",
                            "ats",
                            "banking",
                            "cms",
                            "communication",
                            "crm",
                            "design",
                            "dev-tools",
                            "e-commerce",
                            "erp",
                            "gaming",
                            "hr",
                            "invoicing",
                            "knowledge-base",
                            "legal",
                            "marketing",
                            "other",
                            "payment",
                            "popular",
                            "productivity",
                            "search",
                            "social",
                            "sports",
                            "storage",
                            "support",
                            "surveys",
                            "ticketing",
                            "video"
                        ]
                    }
                },
                "connection_configuration": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "decode_url": {
                    "type": "boolean"
                },
                "default_scopes": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "disable_pkce": {
                    "type": "boolean"
                },
                "docs": {
                    "type": "string"
                },
                "docs_connect": {
                    "type": "string"
                },
                "post_connection_script": {
                    "type": "string"
                },
                "proxy": {
                    "type": "object",
                    "additionalProperties": false,
                    "required": ["base_url"],
                    "properties": {
                        "base_url": {
                            "type": "string"
                        },
                        "connection_config": {
                            "type": "object",
                            "additionalProperties": true
                        },
                        "decompress": {
                            "type": "boolean"
                        },
                        "headers": {
                            "type": "object",
                            "additionalProperties": false,
                            "patternProperties": {
                                "^[a-z0-9_-]+$": {
                                    "type": "string"
                                }
                            }
                        },
                        "paginate": {
                            "type": "object",
                            "additionalProperties": false,
                            "properties": {
                                "cursor_name_in_request": {
                                    "type": "string"
                                },
                                "cursor_path_in_response": {
                                    "type": "string"
                                },
                                "limit_name_in_request": {
                                    "type": "string"
                                },
                                "link_rel_in_response_header": {
                                    "type": "string"
                                },
                                "link_path_in_response_body": {
                                    "type": "string"
                                },
                                "offset_name_in_request": {
                                    "type": "string"
                                },
                                "offset_start_value": {
                                    "type": "number"
                                },
                                "offset_calculation_method": {
                                    "type": "string",
                                    "enum": ["per-page", "by-response-size"]
                                },
                                "response_path": {
                                    "type": "string"
                                },
                                "type": {
                                    "type": "string",
                                    "enum": ["link", "cursor", "offset"]
                                }
                            }
                        },
                        "query": {
                            "type": "object",
                            "additionalProperties": true
                        },
                        "retry": {
                            "type": "object",
                            "additionalProperties": false,
                            "properties": {
                                "after": {
                                    "type": "string",
                                    "pattern": "^[a-z0-9-]+$"
                                },
                                "at": {
                                    "type": "string",
                                    "pattern": "^[a-z0-9-]+$"
                                },
                                "remaining": {
                                    "type": "string",
                                    "pattern": "^[a-z0-9-]+$"
                                },
                                "error_code": {
                                    "type": "number"
                                }
                            }
                        },
                        "verification": {
                            "type": "object",
                            "additionalProperties": false,
                            "required": ["endpoint"],
                            "properties": {
                                "method": {
                                    "type": "string",
                                    "enum": ["GET", "POST"]
                                },
                                "endpoint": {
                                    "type": "string"
                                },
                                "base_url_override": {
                                    "type": "string"
                                },
                                "headers": {
                                    "type": "object",
                                    "additionalProperties": true,
                                    "properties": {
                                        "^[a-zA-Z0-9-]+$": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "token": {
                    "type": "object",
                    "properties": {
                        "expires_in_ms": {
                            "type": "number"
                        },
                        "headers": {
                            "type": "object",
                            "properties": {
                                "alg": {
                                    "type": "string"
                                }
                            }
                        },
                        "payload": {
                            "type": "object",
                            "properties": {
                                "aud": {
                                    "type": "string"
                                }
                            }
                        }
                    }
                },
                "redirect_uri_metadata": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "refresh_url": {
                    "type": "string"
                },
                "refresh_params": {
                    "type": "object",
                    "additionalProperties": true,
                    "properties": {
                        "grant_type": {
                            "type": "string",
                            "enum": ["refresh_token"]
                        }
                    }
                },
                "request_params": {
                    "type": "object",
                    "patternProperties": {
                        "^[a-zA-Z0-9-]+$": {
                            "type": "string"
                        }
                    }
                },
                "request_url": {
                    "type": "string"
                },
                "additional_steps": {
                    "type": "array",
                    "additionalProperties": true,
                    "properties": {
                        "body_format": {
                            "type": "string",
                            "enum": ["form"]
                        },
                        "token_params": {
                            "type": "object",
                            "properties": {
                                "grant_type": {
                                    "type": "string",
                                    "enum": ["urn:salesforce:grant-type:external:cdp"]
                                },
                                "subject_token": {
                                    "type": "string"
                                },
                                "subject_token_type": {
                                    "type": "string",
                                    "enum": ["urn:ietf:params:oauth:token-type:access_token"]
                                }
                            },
                            "required": ["grant_type", "subject_token", "subject_token_type"]
                        },
                        "token_headers": {
                            "type": "object",
                            "properties": {
                                "content-type": {
                                    "type": "string",
                                    "enum": ["application/x-www-form-urlencoded"]
                                }
                            },
                            "required": ["content-type"]
                        },
                        "token_url": {
                            "type": "string"
                        }
                    },
                    "required": ["body_format", "token_params", "token_headers", "token_url"]
                },
                "scope_separator": {
                    "type": "string"
                },
                "signature_method": {
                    "type": "string",
                    "enum": ["HMAC-SHA1", "PLAINTEXT"]
                },
                "signature": {
                    "type": "object",
                    "properties": {
                        "protocol": {
                            "type": "string",
                            "enum": ["WSSE"]
                        }
                    },
                    "required": ["protocol"]
                },
                "token_expiration_buffer": {
                    "type": "number"
                },
                "token_params": {
                    "type": "object",
                    "additionalProperties": true,
                    "properties": {
                        "grant_type": {
                            "type": "string",
                            "enum": [
                                "authorization_code",
                                "client_credentials",
                                "urn:ietf:params:oauth:grant-type:saml2-bearer",
                                "urn:ietf:params:oauth:grant-type:jwt-bearer"
                            ]
                        },
                        "request": {
                            "anyOf": [
                                {
                                    "type": "string"
                                },
                                {
                                    "type": "object"
                                }
                            ]
                        }
                    }
                },
                "token_headers": {
                    "type": "object",
                    "additionalProperties": true
                },
                "token_response": {
                    "type": "object",
                    "additionalProperties": true,
                    "properties": {
                        "token": {
                            "type": "string"
                        },
                        "token_expiration": {
                            "type": "string"
                        },
                        "token_expiration_strategy": {
                            "type": "string"
                        }
                    }
                },
                "token_expires_in_ms": {
                    "type": "number"
                },
                "token_request_auth_method": {
                    "type": "string",
                    "enum": ["basic", "custom"]
                },
                "expires_in_unit": {
                    "type": "string",
                    "enum": ["milliseconds"]
                },
                "token_response_metadata": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "token_url": {
                    "oneOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "object",
                            "additionalProperties": false,
                            "properties": {
                                "API_KEY": {
                                    "type": "string"
                                },
                                "APP": {
                                    "type": "string"
                                },
                                "APP_STORE": {
                                    "type": "string"
                                },
                                "BASIC": {
                                    "type": "string"
                                },
                                "NONE": {
                                    "type": "string"
                                },
                                "OAUTH1": {
                                    "type": "string"
                                },
                                "OAUTH2": {
                                    "type": "string"
                                },
                                "CUSTOM": {
                                    "type": "string"
                                }
                            }
                        }
                    ]
                },
                "token_url_skip_encode": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "webhook_user_defined_secret": {
                    "type": "boolean"
                },
                "webhook_routing_script": {
                    "type": "string"
                },
                "authorization_url_fragment": {
                    "type": "string"
                },
                "connection_config": {
                    "type": "object",
                    "additionalProperties": false,
                    "patternProperties": {
                        "^[a-zA-Z_]+$": {
                            "type": "object",
                            "additionalProperties": false,
                            "required": ["type", "title", "description"],
                            "properties": {
                                "type": {
                                    "type": "string",
                                    "enum": ["string"]
                                },
                                "title": {
                                    "type": "string"
                                },
                                "description": {
                                    "type": "string"
                                },
                                "example": {
                                    "type": "string"
                                },
                                "pattern": {
                                    "type": "string"
                                },
                                "format": {
                                    "type": "string",
                                    "enum": ["hostname", "uri", "uuid", "email"]
                                },
                                "order": {
                                    "type": "number"
                                },
                                "secret": {
                                    "type": "boolean"
                                },
                                "prefix": {
                                    "type": "string"
                                },
                                "suffix": {
                                    "type": "string"
                                },
                                "optional": {
                                    "type": "boolean"
                                },
                                "doc_section": {
                                    "type": "string",
                                    "pattern": "^#[a-z0-9-]+$"
                                },
                                "automated": {
                                    "type": "boolean"
                                }
                            }
                        }
                    }
                },
                "credentials": {
                    "type": "object",
                    "additionalProperties": false,
                    "patternProperties": {
                        "^[a-zA-Z_]+$": {
                            "type": "object",
                            "additionalProperties": false,
                            "required": ["type", "title", "description"],
                            "properties": {
                                "type": {
                                    "type": "string",
                                    "enum": ["string"]
                                },
                                "title": {
                                    "type": "string"
                                },
                                "description": {
                                    "type": "string"
                                },
                                "example": {
                                    "type": "string"
                                },
                                "pattern": {
                                    "type": "string"
                                },
                                "format": {
                                    "type": "string",
                                    "enum": ["hostname", "uri", "uuid", "email"]
                                },
                                "default_value": {
                                    "type": "string"
                                },
                                "secret": {
                                    "type": "boolean"
                                },
                                "hidden": {
                                    "type": "boolean"
                                },
                                "suffix": {
                                    "type": "string"
                                },
                                "doc_section": {
                                    "type": "string",
                                    "pattern": "^#[a-z0-9-]+$"
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
