{
  "name": "Online.net",
  "config": {
    "authorizationURL": "https://console.online.net/oauth/v2/auth",
    "tokenURL": "https://console.online.net/oauth/v2/token",
    "authType": "OAUTH2",
    "tokenParams": { "grant_type": "authorization_code", "redirect_uri": "https://int.bearer.sh/v2/auth/callback" },
    "authorizationParams": { "redirect_uri": "https://int.bearer.sh/v2/auth/callback", "state": 1 },
    "config": { "scope": [] },
    "provider": "Online.net",
    "hint": "You will find your Online.net credentials here: https://console.online.net/en/api/apps"
  },
  "request": {
    "baseURL": "https://api.online.net/api/v1",
    "headers": {
      "Accept": "application/json",
      "Authorization": "Bearer ${auth.accessToken}",
      "User-Agent": "Bearer.sh"
    }
  }
}
