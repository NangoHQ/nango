    "paths": {
        "/": {
            "get": {
                "responses": {
                    "200": {
                        "description": "Default Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "success": {
                                            "type": "boolean"
                                        }
                                    },
                                    "required": ["success"],
                                    "additionalProperties": false
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Default Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ResponseBadRequest"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/v2/integrations/": {
            "get": {
                "operationId": "listIntegrations",
                "summary": "List all integrations",
                "tags": ["Integrations"],
                "description": "List all integrations",
                "parameters": [
                    {
                        "schema": {
                            "default": 10,
                            "type": "number",
                            "minimum": 1,
                            "maximum": 100
                        },
                        "in": "query",
                        "name": "limit",
                        "required": false
                    }
                ],
                "security": [
                    {
                        "secretKey": []
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Default Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "success": {
                                            "type": "boolean"
                                        },
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/Integration"
                                            }
                                        }
                                    },
                                    "required": ["success", "data"],
                                    "additionalProperties": false
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Default Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ResponseBadRequest"
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Default Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ResponseNotFound"
                                }
                            }
                        }
                    },
                    "500": {
                        "description": "Default Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ResponseServerError"
                                }
                            }
                        }
                    }
                }
            }
        },
        "/v2/integrations/{uniqueName}/": {
            "get": {
                "operationId": "getIntegration",
                "summary": "Get an integration by its unique name",
                "tags": ["Integrations"],
                "description": "Get an integration by its unique name",
                "parameters": [
                    {
                        "schema": {
                            "type": "string"
                        },
                        "in": "path",
                        "name": "uniqueName",
                        "required": true
                    }
                ],
                "security": [
                    {
                        "secretKey": []
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Default Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "success": {
                                            "type": "boolean"
                                        },
                                        "data": {
                                            "$ref": "#/components/schemas/Integration"
                                        }
                                    },
                                    "required": ["success", "data"],
                                    "additionalProperties": false
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Default Response",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "#/components/schemas/ResponseBadRequest"
                                }
                            }
                        }
                    },
                    "404": {
