                                "type": "string"
                            },
                            "validation": {
                                "type": "array",
                                "items": {
                                    "type": "object",
                                    "properties": {
                                        "keyword": {
                                            "type": "string"
                                        },
                                        "instancePath": {
                                            "type": "string"
                                        },
                                        "schemaPath": {
                                            "type": "string"
                                        },
                                        "message": {
                                            "type": "string"
                                        },
                                        "params": {
                                            "type": "object",
                                            "properties": {}
                                        }
                                    },
                                    "required": ["keyword", "instancePath", "schemaPath", "message"],
                                    "additionalProperties": {}
                                }
                            }
                        },
                        "required": ["code", "message", "validation"],
                        "additionalProperties": false
                    }
                },
                "required": ["error"],
                "additionalProperties": false
            },
            "IntegrationInput": {
                "type": "object",
                "properties": {
                    "uniqueName": {
                        "type": "string"
                    },
                    "displayName": {
                        "type": "string"
                    },
                    "provider": {
                        "type": "string"
                    },
                    "logoUrl": {
                        "type": "string"
                    },
                    "createdAt": {
                        "type": "string",
                        "format": "date-time",
                        "pattern": "^(?:(?:\\d\\d[2468][048]|\\d\\d[13579][26]|\\d\\d0[48]|[02468][048]00|[13579][26]00)-02-29|\\d{4}-(?:(?:0[13578]|1[02])-(?:0[1-9]|[12]\\d|3[01])|(?:0[469]|11)-(?:0[1-9]|[12]\\d|30)|(?:02)-(?:0[1-9]|1\\d|2[0-8])))T(?:(?:[01]\\d|2[0-3]):[0-5]\\d(?::[0-5]\\d(?:\\.\\d+)?)?(?:Z))$"
                    },
                    "updatedAt": {
                        "type": "string",
                        "format": "date-time",
                        "pattern": "^(?:(?:\\d\\d[2468][048]|\\d\\d[13579][26]|\\d\\d0[48]|[02468][048]00|[13579][26]00)-02-29|\\d{4}-(?:(?:0[13578]|1[02])-(?:0[1-9]|[12]\\d|3[01])|(?:0[469]|11)-(?:0[1-9]|[12]\\d|30)|(?:02)-(?:0[1-9]|1\\d|2[0-8])))T(?:(?:[01]\\d|2[0-3]):[0-5]\\d(?::[0-5]\\d(?:\\.\\d+)?)?(?:Z))$"
                    }
                },
                "required": ["uniqueName", "displayName", "provider", "logoUrl", "createdAt", "updatedAt"],
                "additionalProperties": false
            },
