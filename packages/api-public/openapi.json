{
    "openapi": "3.0.0",
    "info": {
        "title": "Nango API",
        "description": "Nango API specs used to authorize & sync data with external APIs.",
        "version": "2.0.0"
    },
    "components": {
        "securitySchemes": {
            "secretKey": {
                "type": "apiKey",
                "name": "secretKey",
                "in": "header"
            }
        },
