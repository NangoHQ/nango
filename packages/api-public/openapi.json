            "ResponseUnauthorizedInput": {
                "type": "object",
                "properties": {
                    "error": {
                        "type": "object",
                        "properties": {
                            "code": {
                                "type": "string",
                                "const": "unauthorized"
                            },
                            "message": {
                                "type": "string"
                            }
