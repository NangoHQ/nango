{
    "name": "@nangohq/lambda",
    "version": "1.0.0",
    "description": "Lambda functions for Nango",
    "type": "module",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "private": true,
    "scripts": {
        "ts-build": "tsc",
        "build:docker": "cd ../.. && docker buildx build --provenance=false --platform linux/arm64 -f packages/lambda/Dockerfile.lambda -t $LAMBDA_REPOSITORY_NAME .",
        "build:docker:tag": "docker tag $LAMBDA_REPOSITORY_NAME $LAMBDA_REPOSITORY_URI/$LAMBDA_REPOSITORY_NAME:$LAMBDA_IMAGE_TAG",
        "build:docker:push": "docker push $LAMBDA_REPOSITORY_URI/$LAMBDA_REPOSITORY_NAME:$LAMBDA_IMAGE_TAG",
        "build:docker:all": "npm run build:docker && npm run build:docker:tag && npm run build:docker:push"
    },
    "devDependencies": {
        "@types/aws-lambda": "8.10.158",
        "@types/node": "24.10.0",
        "@types/unzipper": "0.10.11",
        "typescript": "5.9.3"
    },
    "dependencies": {
        "@nangohq/kvstore": "file:../kvstore",
        "@nangohq/node": "file:../node-client",
        "@nangohq/runner": "file:../runner",
        "@nangohq/types": "file:../types",
        "@nangohq/utils": "file:../utils",
        "botbuilder": "4.23.3",
        "datadog-lambda-js": "12.130.0",
        "dd-trace": "5.80.0",
        "soap": "1.6.0",
        "unzipper": "0.12.3",
        "zod": "4.1.12"
    }
}
