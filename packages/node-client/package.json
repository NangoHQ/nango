    "keywords": [],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NangoHQ/nango.git",
        "directory": "packages/node-client"
    },
    "license": "SEE LICENSE IN LICENSE FILE IN GIT REPOSITORY",
    "dependencies": {
        "@nangohq/types": "0.53.2",
        "axios": "^1.7.9"
    },
    "engines": {
        "node": ">=18.0"
    },
    "files": [
        "dist/**/*",
        "!**/*.json",
        "README.md"
    ],
    "devDependencies": {
        "tsup": "^8.2.4",
        "vitest": "2.1.9"
    }
