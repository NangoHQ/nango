{
    "compilerOptions": {
        "declaration": true,
        "emitDeclarationOnly": true,
        "outDir": "cjs",
        "module": "esnext",
        "target": "es2022",
        "moduleResolution": "node",
        "verbatimModuleSyntax": false,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "resolveJsonModule": true
    },
    "include": ["lib/scripts.ts"]
}
